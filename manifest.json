{
  "name": "yTranslate",
  "version": "0.1",
  "permissions": ["storage"],
  "options_page": "options.html",
  "page_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "http://*/*","https://*/*"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ]
}